<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>UyTubeWeb</display-name>
	<welcome-file-list>
		<welcome-file>home.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>AltaUsuarioServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.AltaUsuarioServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BusquedaServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.BusquedaServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CategoriaServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.CategoriaServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DatosPruebaServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.DatosPruebaServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.HomeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListaReproduccionServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.ListaReproduccionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UsuarioServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.UsuarioServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>VideoServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.VideoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CompartirServlet</servlet-name>
		<servlet-class>uytubeWeb.servlets.CompartirServlet</servlet-class>
	</servlet>
	<servlet-mapping>
        <servlet-name>ListaReproduccionServlet</servlet-name>
        <url-pattern>/addVidPlaylist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DatosPruebaServlet</servlet-name>
        <url-pattern>/cargarDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoriaServlet</servlet-name>
        <url-pattern>/consult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/dislikeVideo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/follow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/history</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/casa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/leaveFollow</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/likeVideo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoriaServlet</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaReproduccionServlet</servlet-name>
        <url-pattern>/modifyPlaylist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/modifyUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/modifyVideo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/newComment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaReproduccionServlet</servlet-name>
        <url-pattern>/newPlaylist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/newResponse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AltaUsuarioServlet</servlet-name>
        <url-pattern>/newUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/newVideo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaReproduccionServlet</servlet-name>
        <url-pattern>/playlist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaReproduccionServlet</servlet-name>
        <url-pattern>/removeVidPlaylist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/responseComment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/watch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VideoServlet</servlet-name>
        <url-pattern>/v/</url-pattern>
    </servlet-mapping>
    
	
</web-app>